<html>

<div class="jumbotron">
  <h1>gCamp</h1>
  <p>Your life, organized.</p>
</div>

<div class="row">
  <div class="col-md-4">
    <p class="lead">
      <%= image_tag("8969237967_c5960fc544_b.jpg", class: "img-responsive") %>
    </p>
    <div class="panel panel-primary">
      <div class="panel-heading">Tasks</div>
      <div class="panel-body home-panel">
        <p>Grouped by projects and lists, just the way you like'em</p>
      </div>
    </div>
  </div>
  <div class="col-md-4">
    <p class="lead">
      <%= image_tag("8864977481_713b7cbda5_b.jpg", class: "img-responsive") %>
    </p>
    <div class="panel panel-primary">
      <div class="panel-heading">Documents</div>
      <div class="panel-body home-panel">
        <ul>
          <li>Upload</li>
          <li>Comment</li>
          <li>Revise</li>
        </ul>
      </div>
    </div>
  </div>
  <div class="col-md-4">
    <p class="lead">
      <%= image_tag("8203794182_8004f8f636_b.jpg", class: "img-responsive") %>
    </p>
    <div class="panel panel-primary">
      <div class="panel-heading">Comments</div>
      <div class="panel-body home-panel">
        <ul>
          <li>Comment on tasks and documents</li>
          <li>Get email notifications</li>
        </ul>
      </div>
    </div>
  </div>
</div>

<% @quotes.each do |quote| %>
<p><%=quote[0] %></p>
<p><em class="text-muted">- <%=quote[1] %></em></p>
<% end %>

</html>
